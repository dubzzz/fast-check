{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.publish.types.json"
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "mainEntryPointFilePath": "lib/types/fast-check-default.d.ts",
  "dtsRollup": {
    "enabled": false
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "docs/fast-check.api.json"
  },
  "apiReport": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "error"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "error"
      },
      "ae-unresolved-link": {
        "logLevel": "warning"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "error"
      }
    }
  }
}
