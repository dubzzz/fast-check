{
  "compilerOptions": {
    "noEmit": true,
    "declaration": true,
    "sourceMap": false,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "module": "commonjs",
    "target": "es2017",
    "outDir": "lib/"
  }
}
