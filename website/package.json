{
  "private": true,
  "name": "website",
  "scripts": {
    "docusaurus": "docusaurus",
    "sync:sponsors": "[ -f static/img/sponsors.svg ] || curl -o static/img/sponsors.svg https://raw.githubusercontent.com/dubzzz/sponsors-svg/main/sponsorkit/sponsors.svg",
    "start": "pnpm run sync:sponsors && cp ../.all-contributorsrc src/components/HomepageContributors/all-contributors.json && node prebuild/optimize-images.mjs && docusaurus start",
    "build": "pnpm run sync:sponsors && cp ../.all-contributorsrc src/components/HomepageContributors/all-contributors.json && node prebuild/optimize-images.mjs && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.20.0",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@docusaurus/core": "3.9.2",
    "@docusaurus/faster": "3.9.2",
    "@docusaurus/plugin-pwa": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@docusaurus/remark-plugin-npm2yarn": "3.9.2",
    "@docusaurus/theme-mermaid": "3.9.2",
    "clsx": "^2.1.1",
    "lodash": "^4.17.23",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/theme-classic": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@jest/globals": "^30.2.0",
    "@mdx-js/react": "^3.1.1",
    "@types/lodash": "^4.17.23",
    "@types/node": "^24.10.7",
    "@types/react": "^19.2.13",
    "docusaurus-plugin-llms": "^0.2.2",
    "fast-check": "workspace:*",
    "glob": "^13.0.0",
    "jest": "^30.2.0",
    "jimp": "^1.6.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
