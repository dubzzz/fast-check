{
  "name": "fast-check",
  "version": "1.2.2",
  "description": "Property based testing framework in TypeScript (like QuickCheck)",
  "main": "lib/fast-check.js",
  "types": "lib/fast-check.d.ts",
  "scripts": {
    "prebuild": "ts-node prebuild/prebuild.ts",
    "build": "tsc",
    "build:publish": "tsc -p tsconfig.publish.json",
    "webbuild": "browserify lib/fast-check.js --s fastcheck | uglifyjs -cm > lib/bundle.js",
    "watch": "tsc -w",
    "test": "npm run build && nyc mocha \"test/unit/**/*.spec.ts\"",
    "e2e": "mocha \"test/e2e/**/*.spec.ts\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "typedoc --options typedoc.json ./src/",
    "format:check": "prettier --list-different \"**/*.ts\"",
    "format:fix": "prettier --write \"**/*.ts\"",
    "lint:check": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dubzzz/fast-check.git"
  },
  "author": "Nicolas DUBIEN <github@dubien.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dubzzz/fast-check/issues"
  },
  "homepage": "https://github.com/dubzzz/fast-check#readme",
  "dependencies": {
    "core-js": "^2.5.5",
    "lorem-ipsum": "^1.0.4",
    "pure-rand": "^1.1.1"
  },
  "devDependencies": {
    "@types/lorem-ipsum": "^1.0.2",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.3",
    "browserify": "^16.2.0",
    "coveralls": "^3.0.0",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "prettier": "^1.12.1",
    "source-map-support": "^0.5.4",
    "ts-node": "^6.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.1",
    "uglifyjs": "^2.4.11"
  },
  "keywords": [
    "property-based testing",
    "end-to-end testing",
    "ui testing",
    "testing",
    "quickcheck",
    "quality-assurance"
  ]
}
